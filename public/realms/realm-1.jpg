<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="400" height="200" viewBox="0 0 400 200">
  <!-- Background gradient for Realm 1 - Marketplace -->
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#92400E;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#713F12;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#grad1)" />
  
  <!-- Marketplace elements -->
  <g opacity="0.7">
    <!-- Stylized marketplace stalls -->
    <rect x="50" y="100" width="60" height="50" fill="#F59E0B" rx="4" ry="4" />
    <rect x="120" y="100" width="60" height="50" fill="#F59E0B" rx="4" ry="4" />
    <rect x="190" y="100" width="60" height="50" fill="#F59E0B" rx="4" ry="4" />
    <rect x="260" y="100" width="60" height="50" fill="#F59E0B" rx="4" ry="4" />
    
    <!-- Awnings -->
    <path d="M40,100 L70,80 L120,80 L110,100 Z" fill="#B45309" />
    <path d="M110,100 L120,80 L190,80 L180,100 Z" fill="#B45309" />
    <path d="M180,100 L190,80 L260,80 L250,100 Z" fill="#B45309" />
    <path d="M250,100 L260,80 L330,80 L320,100 Z" fill="#B45309" />
    
    <!-- People silhouettes -->
    <circle cx="70" cy="120" r="10" fill="#78350F" />
    <circle cx="145" cy="120" r="10" fill="#78350F" />
    <circle cx="220" cy="120" r="10" fill="#78350F" />
    <circle cx="295" cy="120" r="10" fill="#78350F" />
  </g>
  
  <!-- Coins -->
  <circle cx="100" cy="50" r="15" fill="#FBBF24" stroke="#F59E0B" stroke-width="2" />
  <circle cx="300" cy="50" r="15" fill="#FBBF24" stroke="#F59E0B" stroke-width="2" />
  <text x="100" y="55" font-family="sans-serif" font-size="15" text-anchor="middle" fill="#92400E">₿</text>
  <text x="300" y="55" font-family="sans-serif" font-size="15" text-anchor="middle" fill="#92400E">₿</text>
  
  <!-- Realm name -->
  <text x="200" y="40" font-family="serif" font-size="20" text-anchor="middle" fill="#FBBF24">The Marketplace</text>
</svg>